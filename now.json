{
    "version": 2,
    "name": "go",
    "builds": [
        { "src": "example/*.go", "use": "@now/go" }
    ],
    "routes": [
        { "src": "/api/greeting", "dest": "/example/greeting.go" },
        { "src": "/api/version", "dest": "/example/version.go" },
        { "src": "/api/list", "dest": "/yuque/list.go" },
        { "src": "/api/details", "dest": "/yuque/details.go" },
        { "src": "/api/repo", "dest": "/yuque/repo.go" }
    ]
}
