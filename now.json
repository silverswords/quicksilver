{
    "version": 2,
    "name": "yuque",
    "builds": [
        { "src": "/*.go", "use": "@now/go" }
    ],
    "routes": [
        { "src": "/api/v1/yuque/list", "dest": "/list.go" },
        { "src": "/api/v1/yuque/details", "dest": "/details.go" },
        { "src": "/api/v1/yuque/repo", "dest": "/repo.go" }
    ]
}
